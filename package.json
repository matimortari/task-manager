{
	"name": "task-manager",
	"version": "0.1.0",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "prisma generate && next build",
		"start": "next start",
		"lint": "eslint .",
		"db:push": "dotenv -e .env.local -- prisma db push",
		"db:dev": "dotenv -e .env.local -- prisma migrate dev",
		"db:deploy": "dotenv -e .env.production -- prisma migrate deploy",
		"db:studio": "dotenv -e .env.local -- prisma studio"
	},
	"dependencies": {
		"@iconify/react": "5.2.0",
		"@next-auth/prisma-adapter": "1.0.7",
		"@prisma/client": "6.5.0",
		"@tanstack/react-query": "5.69.0",
		"@vercel/analytics": "1.5.0",
		"moment": "2.30.1",
		"next": "15.2.3",
		"next-auth": "4.24.11",
		"next-themes": "0.4.6",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"react-hook-form": "7.54.2",
		"react-hot-toast": "2.5.2",
		"recharts": "2.15.1"
	},
	"devDependencies": {
		"@eslint/compat": "1.2.7",
		"@eslint/js": "9.23.0",
		"@tanstack/react-query-devtools": "5.69.0",
		"@types/node": "22.13.11",
		"@types/react": "19.0.12",
		"@types/react-dom": "19.0.4",
		"autoprefixer": "10.4.21",
		"dotenv-cli": "8.0.0",
		"eslint-plugin-react": "7.37.4",
		"eslint-plugin-tailwindcss": "3.18.0",
		"globals": "16.0.0",
		"postcss": "8.5.3",
		"prisma": "6.5.0",
		"tailwindcss": "3.4.17",
		"typescript": "5.8.2",
		"typescript-eslint": "8.27.0"
	}
}
